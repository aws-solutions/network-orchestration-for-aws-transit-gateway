{
  "name": "network-orchestrator-for-aws-transit-gateway",
  "version": "3.3.3",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^5.4.0",
    "@cloudscape-design/collection-hooks": "^1.0.19",
    "@cloudscape-design/components": "3.0.194",
    "@cloudscape-design/design-tokens": "^3.0.3",
    "@cloudscape-design/global-styles": "^1.0.7",
    "@types/node": "^18.18.4",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "aws-amplify": "^5.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "babel-jest": "^29.5.0",
    "msw": "^1.3.2"
  },

  "jest": {
    "testMatch": [
      "**/*.test.ts(x)?"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      ".+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@cloudscape-design/components)/"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts*",
      "!<rootDir>/node_modules/",
      "!/build/"
    ]
  },
  "overrides": {
    "svgo": {
      "css-select": "~4.3.0"
    },
    "fast-xml-parser": "4.2.5",
    "resolve-url-loader": {
      "postcss": "8.4.31"
    }
  }
}
